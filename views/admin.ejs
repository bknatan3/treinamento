<!DOCTYPE html>
<html lang="pt-BR">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Administração</title>
</head>
<body>
    <header>
        <h1>Painel Administrativo</h1>
        <a href="/">Home</a>
        <a href="/logout">Sair</a>
    </header>
    
    <h2>Lista de Usuários</h2>
    <table>
        <tr>
            <th>Nome</th>
            <th>Email</th>
            <th>Cidade</th>
            <th>Tipo</th>
        </tr>
        <% usuarios.forEach(usuario => { %>
            <tr>
                <td><%= usuario.nome %></td>
                <td><%= usuario.email %></td>
                <td><%= usuario.cidade %></td>
                <td><%= usuario.tipo %></td>
            </tr>
        <% }) %>
    </table>
</body>
</html>
